<div class="form-group row">
    <label class="col-form-label col-sm-2" for="Title">Título</label>
    <div class="col-sm-8">
      <input readonly class="form-control" id="Title" value="<%= model.Title %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Owner">Proprietário do Vídeo</label>
    <div class="col-sm-7">
      <input readonly class="form-control" id="Owner" value="<%= model.Owner %>" />
    </div>
  </div>
  
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Url">Proprietário do Vídeo</label>
    <div class="col-sm-7">
      <video width="250" controls>
        <source src="/video/<%=model.Url%>" type="video/mp4">
        <source src="/video/<%=model.Url%>" type="video/webm"> 
      </video>

    </div>
  </div>
  
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Comments">Comentários</label>
    <div class="col-sm-10">
      <textarea readonly class="form-control" 
                cols="20" id="Comments" 
                maxlength="32000" 
                rows="7"><%= model.Comments %></textarea>
    </div>
  </div>

  
  