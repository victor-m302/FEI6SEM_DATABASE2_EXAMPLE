<%- include("_header") -%>

<h1>Atualizar Audio</h1>

<form action="/audio/edit_audio/<%= model.Audio_ID %>" method="post" enctype="multipart/form-data">
  <div class="form-horizontal">

    <%- include("_editor_audio") -%>

    <div class="form-group row">
      <label class="col-form-label col-sm-2"></label>
      <div class="col-sm-10">
        <input type="submit" value="Update" class="btn btn-default btn-warning" />
        <a class="btn btn-outline-dark cancel" href="/audio">Cancel</a>
      </div>
    </div>
  </div>
</form>

<%- include("_footer") -%>