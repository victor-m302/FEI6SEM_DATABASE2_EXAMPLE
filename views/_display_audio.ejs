

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="Url">Mídia</label>
  <div class="col-sm-8">
    <audio controls><source src="/audio/<%= model.Url %>" type="audio/mpeg"> </audio>
  </div>
</div>

<div class="form-group row">
    <label class="col-form-label col-sm-2" for="Title">Título</label>
    <div class="col-sm-8">
      <input readonly class="form-control" id="Title" value="<%= model.Title %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Artist">Artista</label>
    <div class="col-sm-7">
      <input readonly class="form-control" id="Artist" value="<%= model.Artist %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Comments">Comentários</label>
    <div class="col-sm-10">
      <textarea readonly class="form-control" 
                cols="20" id="Comments" 
                maxlength="32000" 
                rows="7"><%= model.Comments %></textarea>
    </div>
  </div>

  
  