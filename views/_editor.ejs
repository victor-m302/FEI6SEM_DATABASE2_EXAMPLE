<div class="form-group row">
    <label class="col-form-label col-sm-2" for="Title">Título</label>
    <div class="col-sm-8">
      <input autofocus class="form-control" name="Title" value="<%= model.Title %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Author">Autor</label>
    <div class="col-sm-7">
      <input class="form-control" name="Author" value="<%= model.Author %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Comments">Comentários</label>
    <div class="col-sm-10">
      <textarea class="form-control" cols="20" name="Comments" maxlength="32000" rows="7"><%= model.Comments %></textarea>
    </div>
  </div>  

  <div class="form-group row">
    <!--<input type="file" name="teste" value="<%= model.Url %>>-->
    <input type="file" name="Url" value="<%= model.Url %>" >
  </div>
  


  