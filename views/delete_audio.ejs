<%- include("_header") -%>

<h1>Deletar Audio</h1>

<form action="/audio/delete_audio/<%= model.Audio_ID %>" method="post">
  <div class="form-horizontal">

    <%- include("_display_audio") -%>

    <div class="form-group row">
      <label class="col-form-label col-sm-2"></label>
      <div class="col-sm-10">
        <input type="submit" value="Excluir" class="btn btn-default btn-danger" />
        <a class="btn btn-outline-dark cancel" href="/audio">Cancelar</a>
      </div>
    </div>
  </div>
</form>

<%- include("_footer") -%>