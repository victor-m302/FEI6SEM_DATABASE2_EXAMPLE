<%- include("_header") -%>

<h1>Delete a record?</h1>

<form action="/video/delete_video/<%= model.Video_ID %>" method="post">
  <div class="form-horizontal">

    <%- include("_display_video") -%>

    <div class="form-group row">
      <label class="col-form-label col-sm-2"></label>
      <div class="col-sm-10">
        <input type="submit" value="Excluir" class="btn btn-default btn-danger" />
        <a class="btn btn-outline-dark cancel" href="/video">Cancelar</a>
      </div>
    </div>
  </div>
</form>

<%- include("_footer") -%>